<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.szboanda.mobile.decision.zhkb.dao.YwzbDAO">
	<resultMap type="java.util.Map" id="ywspMap">
		<result column="NUM" property="NUM" jdbcType="INTEGER" javaType="int" />
		<result column="SPLXDM" property="SPLXDM" jdbcType="VARCHAR" javaType="string" />
	</resultMap>
	
	<!-- 查询【项目审批】业务方法 -->
	<select id="getXmspInfo" resultType="java.util.Map">
		SELECT COUNT(1) AS NUM,SPLXDM FROM T_SJZX_XMSP GROUP BY SPLXDM
	</select>

</mapper>